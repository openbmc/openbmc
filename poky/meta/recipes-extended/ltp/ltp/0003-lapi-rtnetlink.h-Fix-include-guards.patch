--- a/include/lapi/rtnetlink.h
+++ b/include/lapi/rtnetlink.h
@@ -1,8 +1,8 @@
 // SPDX-License-Identifier: GPL-2.0-or-later
 /* Copyright (c) 2021 Petr Vorel <petr.vorel@gmail.com> */

-#ifndef LAPI_IF_ADDR_H__
-# define LAPI_IF_ADDR_H__
