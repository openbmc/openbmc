/dts-v1/;

/* TCG2 Logs */
/memreserve/ 0x35F00000 0x00010000;

#include "nuvoton-npcm845.dtsi"
#include "nuvoton-npcm845-scm-pincfg.dtsi"

/ {
	model = "Nuvoton npcm845 Development Board";
	compatible = "nuvoton,npcm845";

	aliases {
		serial0 = &serial0;
		serial1 = &serial4;
		serial2 = &serial5;
		ethernet0 = &gmac1;
		spi0 = &fiu0;
		spi3 = &fiu3;
		usb0 = &udc0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		i2c4 = &i2c4;
		i2c5 = &i2c5;
		i2c6 = &i2c6;
		i2c7 = &i2c7;
		i2c8 = &i2c8;
		i2c9 = &i2c9;
		i2c10 = &i2c10;
		i2c11 = &i2c11;
		i2c12 = &i2c12;
		i2c15 = &i2c15;
		i2c19 = &i2c19;

	};

	chosen {
		stdout-path = &serial0;
	};

	memory {
		reg = <0x0 0x0 0x0 0x80000000>;
	};

	vsbv8: vsbv8 {
		compatible = "regulator-npcm845";
		regulator-name = "v8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	tpm@0 {
		compatible = "microsoft,ftpm";
	};

	firmware {
		optee {
			 compatible = "linaro,optee-tz";
			 method = "smc";
		};
	};
};

&serial0 {
	status = "okay";
	assigned-clocks = <&clk NPCM8XX_CLK_UART>,
			  <&clk NPCM8XX_CLK_UART2>;
	assigned-clock-parents = <&clk NPCM8XX_CLK_PLL2DIV2>;
	assigned-clock-rates = <24000000>, <24000000>;
};

&serial4 {
	status = "okay";
	clock-frequency = <24000000>;
};

&serial5 {
	status = "okay";
	clock-frequency = <24000000>;
};

&fiu0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&spi0cs1_pins>;
	spi-nor@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;
	};
	spi_flash@1 {
		compatible = "jedec,spi-nor";
		reg = <1>;
		spi-max-frequency = <50000000>;
	};
};

&fiu3 {
	pinctrl-0 = <&spi3_pins>, <&spi3quad_pins>, <&spi3cs1_pins>;
	status = "okay";
	spi-nor@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;
	};
	spi-nor@1 {
		compatible = "jedec,spi-nor";
		reg = <1>;
		spi-max-frequency = <25000000>;
	};
};

&gmac1 {
	phy-mode = "rgmii-id";
	status = "okay";
};

&sdhci0 {
	index = <0x0>;
	bus-width = <0x8>;
	clocks = <&clk NPCM8XX_CLK_SDHC>;
	clock-frequency = <50000000>;
	vqmmc-supply = <&vsbv8>;
	vqmmc-microvolt = <1800000>;
	status = "okay";
};

&rng {
	status = "okay";
};

&aes {
	status = "okay";
};

&sha {
	status = "okay";
};

&otp {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	status = "okay";
};

&i2c3 {
	status = "okay";
};

&i2c4 {
	status = "okay";
};

&i2c5 {
	status = "okay";
};

&i2c6 {
	status = "okay";
};

&i2c7 {
	status = "okay";
};

&i2c8 {
	status = "okay";
};

&i2c9 {
	status = "okay";
};

&i2c10 {
	status = "okay";
};

&i2c11 {
	status = "okay";
};

&i2c12 {
	status = "okay";
};

&i2c15 {
	status = "okay";
};

&i2c19 {
	status = "okay";
};

&usbphy1 {
	status = "okay";
};

&usbphy2 {
	status = "okay";
};

&udc0 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc1 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc2 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc3 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc4 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc6 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc7 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc8 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&udc9 {
	status = "okay";
	phys = <&usbphy2 2>;
};

&host_intf {
	type = "espi";
	ioaddr = <0x2e>;
	channel-support = <0xf>;
};

&pinctrl {
        pinctrl-names = "default";
        pinctrl-0 = <
