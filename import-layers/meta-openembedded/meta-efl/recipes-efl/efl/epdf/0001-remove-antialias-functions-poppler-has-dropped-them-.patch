 {
    return "0.5";
 }
-
-Eina_Bool epdf_fonts_antialias_get(void)
-{
-   return EINA_TRUE;
-}
-
-void epdf_fonts_antialias_set(Eina_Bool on)
-{
-}
-
-Eina_Bool epdf_lines_antialias_get(void)
-{
-   return EINA_TRUE;
-}
-
-void epdf_lines_antialias_set(Eina_Bool on)
-{
-}
